@page "/"
@attribute [AllowAnonymous]
@inject NavigationManager _navigationManager

<AuthorizeView>
	<Authorized>
		@if (true) { _navigationManager.NavigateTo("dnd"); }
	</Authorized>
	<NotAuthorized>
		@if (true) { _navigationManager.NavigateTo("login"); }
	</NotAuthorized>
</AuthorizeView>
